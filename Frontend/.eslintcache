king\" exact>\r\n        <Rank />\r\n      </Route>\r\n      <Route path=\"/login\" exact>\r\n        <Login />\r\n      </Route>\r\n      <Route path=\"/signup\" exact>\r\n        <Signup />\r\n      </Route>\r\n      <Route path=\"/forgetpass\" exact>\r\n        <Forgetpass />\r\n      </Route>\r\n      <Route path=\"/prevent\" exact>\r\n        <Prevent />\r\n      </Route>\r\n      {/* <Route path=\"/help\" exact>\r\n        <Help />\r\n      </Route> */}\r\n      <Route path=\"/helpnew\" exact>\r\n        <Helpnew />\r\n      </Route>\r\n      <Route path=\"/contractus\" exact>\r\n        <Contractus />\r\n      </Route>\r\n      <Route path=\"/mypost/:uid\" exact>\r\n        <Mypost />\r\n      </Route>\r\n      <Route path=\"/post\" exact>\r\n        <Post />\r\n      </Route>\r\n      <Route path=\"/linkruleshow\" exact>\r\n        <Linkruleshow />\r\n      </Route>\r\n    </Switch>\r\n    </usercontext.Provider>\r\n  </Router>\r\n);\r\n\r\n}\r\nexport default App;","D:\\Programming\\Web Programming\\Project SE\\MonkeyFruad\\Frontend\\src\\user\\pages\\ranking.js",["119","120"],"import React, { useEffect, useState } from \"react\";\r\nimport Navbar from \"../components/navbar\";\r\nconst Rank = () => {\r\n  return (\r\n    <div>\r\n      <Navbar />\r\n      <h1>Ranking</h1>\r\n    </div>\r\n  );\r\n};\r\nexport default Rank;\r\n","D:\\Programming\\Web Programming\\Project SE\\MonkeyFruad\\Frontend\\src\\user\\pages\\editpost.js",["121","122"],"import React, { useEffect, useState } from \"react\";\r\nimport Navbar from \"../components/navbar\";\r\nimport Formedit from \"../components/formedit\"\r\nimport \"./editpost.css\";\r\nconst Editpost = () => {\r\n  return (\r\n    <div>\r\n      <Navbar />\r\n      <h1 className=\"h1-editpost\">แก้ไขโพสต์</h1>\r\n      <Formedit/>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Editpost;\r\n","D:\\Programming\\Web Programming\\Project SE\\MonkeyFruad\\Frontend\\src\\user\\pages\\contractus.js",["123","124"],"import 